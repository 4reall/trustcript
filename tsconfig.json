{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["@emotion/core"],
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/common/components/*"
      ],
      "@/assets/*": [
        "public/assets/*"
      ],
      "@/context/*": [
        "src/common/context/*"
      ],
      "@/hooks/*": [
        "src/common/hooks/*"
      ],
      "@/modules/*": [
        "src/modules/*"
      ],
      "@/types/*": [
        "src/common/types/*"
      ],
      "@/utils/*": [
        "src/common/utils/*"
      ],
      "@/mock/*": [
        "src/common/mock/*"
      ],
      "@/layout/*": [
        "src/common/layout/*"
      ],
      "@/styles/*": [
        "src/common/layout/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}


